{
    "name": "jgalyon/cafsp-widgets",
    "description": "WordPress plugin for CAFSP",
    "type": "wordpress-plugin",
    "license": "GPLv2",
    "authors": [
        {
            "name": "galyonj",
            "email": "galyon.jb@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "installer-paths": {
            "plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "themes/${$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "require": {
        "wpackagist-plugin/debug-bar": "^1.1",
        "wpackagist-plugin/debug-bar-shortcodes": "^2.0",
        "wpackagist-plugin/debug-bar-constants": "^2.0",
        "wpackagist-plugin/debug-bar-actions-and-filters-addon": "^1.5",
        "wpackagist-plugin/debug-bar-list-dependencies": "^1.1",
        "wpackagist-plugin/elementor": "^3.4",
        "wpackagist-plugin/debug-this": "^0.6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "^2.3"
    }
}
